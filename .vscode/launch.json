        {
            "name": "debug-preprocess",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/run/bin_Debug/Preprocess.exe",//程序入口
            "console": "integratedTerminal",//执行cout输出在哪个终端
            "cwd": "${workspaceRoot}/run/bin_Debug",//指定工作目录
            "args": [
                "-d","0",
                "-p","4",
                "-t","1800",
                "--batch",".*0001.*",//批量执行模式, 根据给定的通配符并行执行多个文件
                "--single","alien.obj",//单一执行模式, 只执行一个文件
                "--dataset","PolyAtlas-Sample-30",
                "--use_individual_model_dir",
                "--parallel_level","process"
                ],//参数
            "preLaunchTask": "build-preprocess-debug"//debug前先进行build
        }
    ]
}